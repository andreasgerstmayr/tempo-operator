apiVersion: v1
kind: Namespace
metadata:
  name: kuttl-operator-upgrade
---
apiVersion: operators.coreos.com/v1
kind: OperatorGroup
metadata:
  name: operatorgroup
  namespace: kuttl-operator-upgrade
---
apiVersion: operators.coreos.com/v1alpha1
kind: CatalogSource
metadata:
  name: old-catalog
  namespace: kuttl-operator-upgrade
spec:
  sourceType: grpc
  image: ${UPGRADE_FROM_CATALOG}
---
apiVersion: operators.coreos.com/v1alpha1
kind: CatalogSource
metadata:
  name: new-catalog
  namespace: kuttl-operator-upgrade
spec:
  sourceType: grpc
  image: ${UPGRADE_TO_CATALOG}
